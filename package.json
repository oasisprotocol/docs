{
  "name": "docs-oasis-dev",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --no-open",
    "build": "docusaurus build",
    "diagrams": "find docs -name '*.mmd' -exec mmdc -i {} -c mermaid.config.json ';'",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "test": "jest src",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "blc": "linkinator http://localhost:3000 --config linkinator.config.json",
    "lint-markdown": "markdownlint-cli2 'docs/**/*.md' 'docs/**/*.mdx'",
    "fmt-markdown": "markdownlint-cli2-fix 'docs/**/*.md' 'docs/**/*.mdx'"
  },
  "dependencies": {
    "@docusaurus/core": "^2.4.1",
    "@docusaurus/plugin-client-redirects": "^2.4.1",
    "@docusaurus/preset-classic": "^2.4.1",
    "@easyops-cn/docusaurus-search-local": "^0.34.0",
    "@mdx-js/react": "^1.6.22",
    "@mermaid-js/mermaid-cli": "^9.3.0",
    "clsx": "^1.2.1",
    "linkinator": "^4.1.2",
    "markdownlint-cli2": "^0.6.0",
    "plotly.js-basic-dist": "^2.18.2",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-plotly.js": "^2.5.1"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/plotly.js-basic-dist": "^1.54.0",
    "@types/react-plotly.js": "^2.5.1",
    "jest": "^29.0.3"
  }
}
